  <body>
    <div class="mdl-layout mdl-js-layout">
      <main class="jctf-content mdl-layout__content">
        <div class="jctf-content__cards mdl-grid">
          <div class="jctf-content__auth mdl-cell mdl-cell--12-col mdl-grid">
            <!-- Auth Begin-->
            <div class="jctf-auth mdl-cell mdl-cell--12-col">
              <div id="auth-logo">
                <img id="auth-logo-image" src="images/logo.png">
              </div>
              <form action="auth.php" method="post">
                <div class="mdl-textfield mdl-js-textfield">
                  <input class="mdl-textfield__input" type="text" id="token-input" name="token">
                    <label class="mdl-textfield__label" for="token-input">Access token</label>
                </div>
                <button type="submit" id="flag-button" class="mdl-button mdl-js-button mdl-button--raised mdl-button--accent">
                  Sign in
                </button>
              </form>
            </div>
            <!-- Auth End-->
         </div>
